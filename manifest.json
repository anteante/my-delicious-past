{
	// Required
	"name": "My Delicious Past",
	"version": "0.1",

	// This code uses setPopup(), which was added in Chrome 5.0.316.0.
	// http://code.google.com/chrome/extensions/browserAction.html
	"minimum_chrome_version": "5.0.316.0",

	// Recommended
	"description": "Start of a Chrome extension",
	"icons": {
		"16": "img/icon_16.png",
		"32": "img/icon_32.png",
		"64": "img/icon_64.png",
		"128": "img/icon_128.png"
	},
	// "default_locale": "en",

	// Pick one (or none)
	"browser_action": {
	"default_icon": "img/icon.png",
	       "default_title": "My Delicious Past",	// shown in tooltip (optional)
	       "default_popup": "popup.html"		// popup to show when icon is clicked (optional) 
	},
	// "page_action": {},
	// "theme": {},

	// Add any of these that you need
	"background_page": "background.html",
	"options_page": "options.html",
	
	// "chrome_url_overrides": {...},
	// "content_scripts": [...],
	// "key": "publicKey",
	
	"permissions": [
		"unlimitedStorage",		// Set for unlimited storage
		"tabs",					// for chrome.tabs.onRemoved
		"https://api.del.icio.us/*"
	],
	
	// "plugins": [...],
	// "update_url": "http://path/to/updateInfo.xml"
	"incognito": "spanning"
}