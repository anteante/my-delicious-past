{
	"name": "My Delicious Past",
	"version": "0.2",
	"minimum_chrome_version": "5.0.316.0",
	"description": "Start of a Chrome extension",
	"icons": {
		"16": "img/icon_16.png",
		"32": "img/icon_32.png",
		"64": "img/icon_64.png",
		"128": "img/icon_128.png"
	},
	"browser_action": {
	"default_icon": "img/icon.png",
	       "default_title": "My Delicious Past",
	       "default_popup": "popup.html"
	},
	"background_page": "background.html",
	"options_page": "options.html",
	"permissions": [
		"unlimitedStorage",
		"tabs",
		"https://api.del.icio.us/*"
	],
	"incognito": "spanning"
}